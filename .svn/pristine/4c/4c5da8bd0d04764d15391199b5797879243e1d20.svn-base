package m.w.frs.mgserver.domain;

import java.io.Serializable;

public class DataAdapter implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4953269672913245944L;
	private Long id;
	private String imageUrl;
	private String sortUrl;
	private String sortTitle;
	private String sortTitleHint;
	private String title;
	private String description;
	private String pieceCategory;
	private boolean init = true;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getImageUrl() {
		return imageUrl;
	}

	public void setImageUrl(String imageUrl) {
		this.imageUrl = imageUrl;
	}

	public String getSortTitle() {
		return sortTitle;
	}

	public void setSortTitle(String sortTitle) {
		this.sortTitle = sortTitle;
	}

	public String getSortTitleHint() {
		return sortTitleHint;
	}

	public void setSortTitleHint(String sortTitleHint) {
		this.sortTitleHint = sortTitleHint;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public boolean getInit() {
		return init;
	}

	public void setInit(boolean init) {
		this.init = init;
	}

	public String getSortUrl() {
		return sortUrl;
	}

	public void setSortUrl(String sortUrl) {
		this.sortUrl = sortUrl;
	}

	public String getPieceCategory() {
		return pieceCategory;
	}

	public void setPieceCategory(String pieceCategory) {
		this.pieceCategory = pieceCategory;
	}
	
	


}
