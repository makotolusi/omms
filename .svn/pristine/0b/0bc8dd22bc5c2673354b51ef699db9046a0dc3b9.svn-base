<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/css/*</pattern>
        <pattern>/easyui/*</pattern>
        <pattern>/images/*</pattern>
        <pattern>/js/*</pattern>
        <pattern>/kindeditor/*</pattern>

        <pattern>/upload/*</pattern>

        <pattern>/ztjs/sso/query/userinfo.jsp</pattern>
        <pattern>/login.jsp</pattern>
        <!-- 所有通过Ajax获取div形式打开的页面 -->
        <pattern>/ex/*</pattern>
        <pattern>/wfsqlserver/*</pattern>
        <!-- 所有CMS的页面 -->
        <pattern>/cms/*</pattern>
    </excludes>

    <!-- 主页 -->
    <decorator name="omms-index" page="index.jsp">
        <pattern>/ztjs/omms/index.jsp</pattern>
    </decorator>

    <decorator name="api" page="api.jsp">
        <pattern>/api/*</pattern>
        <pattern>/page/*</pattern>
    </decorator>

    <decorator name="main" page="main.jsp">
        <pattern>/*</pattern>
    </decorator>
</decorators>
