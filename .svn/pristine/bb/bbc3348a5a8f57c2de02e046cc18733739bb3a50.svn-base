package m.w.frs.mgserver.domain;

/**
 * 订单抢购结果
 * @author lusi
 *
 */
public class SnapUpResult {

	public static final String STOCK = "stock";
	public static final String UNPAID = "unpaid";
	public static final String PRICE = "price";
	private SnapUpOrder order;
	private CODE code;//抢购结果
	private int stock;//库存

	public SnapUpResult(SnapUpOrder order) {
		this.order = order;
	}

	public SnapUpResult(CODE code, int stock, SnapUpOrder order) {
		super();
		this.code = code;
		this.stock = stock;
		this.order = order;
	}

	public CODE getCode() {
		return code;
	}

	public void setCode(CODE code) {
		this.code = code;
	}

	public int getStock() {	
		return stock;
	}

	public void setStock(int stock) {
		this.stock = stock;
	}

	public SnapUpOrder getOrder() {
		return order;
	}

	public void setOrder(SnapUpOrder order) {
		this.order = order;
	}

	public enum CODE {
		SUCCESS("抢购成功",100), STOCK_NOT_ENOUGH("该商品库存不足",101), EXCEPTION("抢购异常",102);

		private final String value;
		private final Integer index;
		CODE(String value,Integer index) {
			this.value = value;
			this.index=index;
		}

		public String getValue() {
			return value;
		}
	}
}