		<th style="width:60px">图片URL9</th>
		<td><input name="picUrl9" id="picUrl9" class="easyui-validatebox" value="${obj.picUrl9}" style="width:95%" data-options="validType:'blength[1,300]'"/></td>
	</tr>
    <tr>
		<th style="width:60px">图片URL10</th>
		<td><input name="picUrl10" id="picUrl10" class="easyui-validatebox" value="${obj.picUrl10}" style="width:95%" data-options="validType:'blength[1,300]'"/></td>
	</tr>
	<tr>	
		<th style="width:100px">录入时间</th>
		<td colspan="3">${obj.entertime}</td>
	</tr>	
	<tr>	
		<th style="width:100px">录入人</th>
		<td colspan="3">${obj.username}</td>
	</tr>

</table>

<input type="hidden" name="id" value="${obj.id}"/>
<input type="hidden" name="systemName" value="${obj.systemName}"/>
<input type="hidden" name="userName" value="${obj.username}"/>
<input type="hidden" name="userId" value="${obj.userId}"/>
<input type="hidden" name="entertime" value="${wx:datetime(obj.entertime)}"/>

