managerFactoryClassName=ro.isdc.wro.extensions.manager.ExtensionsConfigurableWroManagerFactory
# Available Processors  
# https://code.google.com/p/wro4j/wiki/AvailableProcessors
preProcessors=cssLint, jsLint, cssImport,cssUrlRewriting,semicolonAppender
postProcessors=yuiCssMin,yuiJsMin, cssLint, jsLint

debug=false
encoding=UTF-8

gzipResources=true
cacheGzippedContent=true
cacheUpdatePeriod=0
modelUpdatePeriod=0
disableCache=false
resourceWatcherUpdatePeriod=0

jmxEnabled=false

ignoreFailingProcessor=true
ignoreMissingResources=true
ignoreEmptyGroup=true

