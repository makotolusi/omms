	</tr>
    <tr>
		<th style="width:60px">图片URL6</th>
		<td><input name="picUrl6" id="picUrl6" class="easyui-validatebox" value="${obj.picUrl6}" style="width:95%" data-options="validType:'blength[1,300]'"/></td>
	</tr>
