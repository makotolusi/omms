<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ include file="/common/include/_includes.jsp"%>
<script type="text/javascript">

</script>
<table class="form-table" style="width:98%">

	<tr>	
		<th style="width:100px">系统名称</th>
		<td colspan="3">${obj.systemName}</td>
	</tr>	
    <tr>
		<th style="width:60px">产品名称</th>
		<td><input name="name" id="name" class="easyui-validatebox" value="${obj.name}" style="width:95%" data-options="required:true,validType:'blength[1,100]'"/></td>
	</tr>
    <tr>
		<th style="width:60px">产品code</th>
		<td><input name="productCode" id="productCode" class="easyui-validatebox" value="${obj.productCode}" style="width:95%" data-options="required:true,validType:'blength[1,100]'"/></td>
	</tr>	
	<tr>
		<th style="width:60px">价格</th>
		<td><input name="price" value="${obj.price}" maxlength="100" style="width:95%" class="easyui-numberbox" /></td>
	</tr>
    <tr>
		<th style="width:60px">产品数量</th>
		<td><input name="quantity" id="quantity" class="easyui-numberbox" value="${obj.quantity}" style="width:95%"/></td>
	</tr>	
    <tr>
		<th style="width:60px">产品总量</th>
		<td><input name="total" id="total" class="easyui-validatebox" value="${obj.total}" style="width:95%" data-options="validType:'blength[1,100]'"/></td>
	</tr>	
    <tr>
		<th style="width:60px">产品描述</th>
		<td><input name="description" id="description" class="easyui-validatebox" value="${obj.description}" style="width:95%" data-options="validType:'blength[1,200]'"/></td>
	</tr>
    <tr>
		<th style="width:60px">用户体验</th>
		<td><input name="experience" id="experience" class="easyui-validatebox" value="${obj.experience}" style="width:95%" data-options="validType:'blength[1,200]'"/></td>
	</tr>
    <tr>
		<th style="width:60px">货币类型</th>
		<td><input name="currency" id="currency" class="easyui-validatebox" value="${obj.currency}" style="width:95%" data-options="validType:'blength[1,200]'"/></td>
	</tr>	
    <tr>
		<th style="width:60px">部件数量</th>
		<td><input name="piece" id="piece" class="easyui-numberbox" value="${obj.piece}" style="width:95%"/></td>
	</tr>	
    <tr>
		<th style="width:60px">部件类别</th>
		<td><input name="pieceCategory" id="pieceCategory" class="easyui-validatebox" value="${obj.pieceCategory}" style="width:95%" data-options="validType:'blength[1,200]'"/></td>
	</tr>
    <tr>
		<th style="width:60px">产地</th>
		<td><input name="addr" id="addr" class="easyui-validatebox" value="${obj.addr}" style="width:95%" data-options="validType:'blength[1,100]'"/></td>
	</tr>
    <tr>
		<th style="width:60px">图片URL1</th>
		<td><input name="picUrl1" id="picUrl1" class="easyui-validatebox" value="${obj.picUrl1}" style="width:95%" data-options="validType:'blength[1,300]'"/></td>
	</tr>
    <tr>
